@{
    Layout = null;

}
<section class="content" >
    <div class="panel panel-primary">
        <div class="panel-heading">
            <span class="panel-icon"><i class="fa fa-info"></i></span>
            <span class="panel-title">About</span>
        </div>
        <div class="panel-body">
            
            <table class="table table-bordered table-hover">
                <tr>
                    <td><strong class="text-primary">System Version.</strong></td>
                    <td><code>version @ViewContext.HttpContext.ApplicationInstance.GetType().BaseType.Assembly.GetName().Version.ToString();</code></td>
                    @*<td>version pace.yearr.month.revision per month</td>*@
                </tr>
            </table>
        </div>
    </div>
       
</section>
